{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "netcoredbg",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/BlazorExperiments2.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": true,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "pipeTransport": {
                "pipeCwd": "${workspaceFolder}",
                "pipeProgram": "bash",
                "pipeArgs": ["-c"],
                "debuggerPath": "${HOME}/.local/netcoredbg",
                "debuggerArgs": ["--interpreter=vscode"],
                "quoteArgs": true
            },
            "env": {
                "DOTNET_ENVIRONMENT": "Development"
            },
            "logging": {
                "moduleLoad": false,
                "diagnosticsLog": {
                    "protocolMessages": true
                }
            }
        }
    ]
}
  